<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.integrationtest.mapper.member.ContactMapper">
    <insert id="requestCall" parameterType="com.example.integrationtest.dto.member.ContactDTO">
        INSERT INTO contact(
                            name,
                            email,
                            phone,
                            message
        ) values (
                  #{name},
                  #{email},
                  #{phone},
                  #{message}
                         )
    </insert>

</mapper>